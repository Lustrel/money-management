{% use "bootstrap_4_layout.html.twig" %}


<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Money Management v0.01a - Cadastro de Usuário{% endblock %}</title>
    {% block stylesheets %}<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">{% endblock %}
</head>
<body>

{% block body %}

    {% block header %}
        {{ parent() }}
    {% endblock header %}

    <div class="container">
        <h1 class="text-center">Cadastro de Usuário</h1>

        <h2 class="text-center">Usuário cadastrado com sucesso!</h2>
    </div>

    {% block footer %}
        {{ parent() }}
    {% endblock footer %}

{% endblock %}

{% block javascripts %}{% endblock %}
</body>
</html>
